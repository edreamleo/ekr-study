def next(self) -&gt; tuple[str, int]:
    while True:
        if not self._text:
            raise StopIteration()
        if "/" not in self._text:
            text = self._text
            self._offset = 0
            self._text = ""
            return text, 0
        pre_text, post_text = self._text.split("/", 1)
        self._text = post_text
        self._offset = 0
        if (
            not pre_text
            or not post_text
            or not pre_text[-1].isalpha()
            or not post_text[0].isalpha()
        ):
            self._text = ""
            self._offset = 0
            return f"{pre_text}/{post_text}", 0
        return pre_text, 0

