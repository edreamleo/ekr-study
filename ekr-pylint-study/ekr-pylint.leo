<t tx="ekr.20250131013600.318">def _check_type_x_is_y(
    self,
    node: nodes.Compare,
    left: nodes.NodeNG,
    operator: str,
    right: nodes.NodeNG,
) -&gt; None:
    """Check for expressions like type(x) == Y."""
    left_func = utils.safe_infer(left.func)
    if not (
        isinstance(left_func, nodes.ClassDef) and left_func.qname() == TYPE_QNAME
    ):
