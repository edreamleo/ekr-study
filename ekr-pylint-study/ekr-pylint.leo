def _parse_rcfile_magic_numbers(parsed_val: str) -&gt; float | str:
    parsed_val = parsed_val.encode().decode("unicode_escape")

    if parsed_val.startswith("'") and parsed_val.endswith("'"):
        return parsed_val[1:-1]

