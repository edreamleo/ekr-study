<t tx="ekr.20250131013600.355">def _raise_name_warning(
    self,
    prevalent_group: str | None,
    node: nodes.NodeNG,
    node_type: str,
    name: str,
    confidence: interfaces.Confidence,
    warning: str = "invalid-name",
