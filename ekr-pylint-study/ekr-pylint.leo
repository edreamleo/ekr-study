    """Format and print messages in the context of the path."""
    messages: list[str] = []
    for msg in self.messages:
        obj = ""
        if msg.obj:
            obj = f":{msg.obj}"
        messages.append(f"{msg.msg_id[0]}:{msg.line:&gt;3}{obj}: {msg.msg}")

