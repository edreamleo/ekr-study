        (UL, ORIGIN, DR),
        (UL, UR, DL, DR),
        (UL, UR, ORIGIN, DL, DR),
        (UL, UR, LEFT, RIGHT, DL, DR),
    ][value - 1]

    arrangement = VGroup(*(
        dot.copy().move_to(square.get_bounding_box_point(vect))
        for vect in edge_group
    ))
    arrangement.space_out_submobjects(dot_coalesce_factor)

    super().__init__(square, arrangement)
