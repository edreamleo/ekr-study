    self.default_a_value = default_a_value

    rgb_kwargs = {"value": self.default_rgb_value, "min_value": 0, "max_value": 255, "step": 1}
    a_kwargs = {"value": self.default_a_value, "min_value": 0, "max_value": 1, "step": 0.04}

