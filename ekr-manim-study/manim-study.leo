    #     lambda n: not self.consider_points_equals(points[n - 1], points[n]),
    #     range(nppc, len(points), nppc)
    # )
    split_indices = [0, *split_indices, len(points)]
    return [
        points[i1:i2]
        for i1, i2 in zip(split_indices, split_indices[1:])
        if (i2 - i1) &gt;= nppc
    ]

