<t tx="ekr.20250122151621.851">def squish_rate_func(
    func: RateFunction,
    a: float = 0.4,
    b: float = 0.6,
) -&gt; RateFunction:
    def result(t: float, *args: Any, **kwargs: Any) -&gt; float:
        if a == b:
            return a

        if t &lt; a:
            new_t = 0.0
        elif t &gt; b:
            new_t = 1.0
        else:
            new_t = (t - a) / (b - a)
        return func(new_t, *args, **kwargs)

    return result


