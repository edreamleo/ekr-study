<t tx="ekr.20250122151620.1414">def complete_p_list(self, p_list):
    new_p_list = list(tuplify(p_list))
    remainder = 1.0 - sum(new_p_list)
    if abs(remainder) &gt; EPSILON:
        new_p_list.append(remainder)
    return new_p_list

