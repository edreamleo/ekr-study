    self,
    source: StringMobject,
    target: StringMobject,
    matched_keys: Iterable[str] = [],
    key_map: dict[str, str] = dict(),
    matched_pairs: Iterable[tuple[VMobject, VMobject]] = [],
    **kwargs,
