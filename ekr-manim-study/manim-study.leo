<t tx="ekr.20250122151620.1578">def __init__(
    self,
    filename_or_array: str | Path | np.ndarray,
    width: float = None,
    height: float = None,
    image_mode: str = "RGBA",
    resampling_algorithm: int = Resampling.BICUBIC,
