    from ..scene.scene import Scene

    def is_child_scene(obj: Any, module: types.ModuleType) -&gt; bool:
        return (
            inspect.isclass(obj)
            and issubclass(obj, Scene)
            and obj != Scene
            and obj.__module__.startswith(module.__name__)
