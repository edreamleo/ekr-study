def hash_seed(self) -&gt; tuple:
    """A unique hash representing the result of the generated
    mobject points.

    Used as keys in the ``SVG_HASH_TO_MOB_MAP`` caching dictionary.
    """
    return (
        self.__class__.__name__,
        self.svg_default,
        self.path_string_config,
        self.file_name,
