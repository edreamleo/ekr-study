<t tx="ekr.20250122151620.2044">def _set_submobjects_from_number(self, number):
    self.number = number
    self.submobjects = []

    num_string = self._get_num_string(number)
    self.add(*(map(self._string_to_mob, num_string)))

    # Add non-numerical bits
    if self.show_ellipsis:
