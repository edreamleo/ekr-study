        &gt;&gt;&gt; from manim import Arrow
        &gt;&gt;&gt; arrow = Arrow(np.array([0, 0, 0]), np.array([2, 2, 0]), buff=0)
        &gt;&gt;&gt; arrow.tip.vector.round(2) + 0.
        array([0.25, 0.25, 0.  ])

