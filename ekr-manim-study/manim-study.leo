    self.play(FadeIn(grupo_secante))

    kwargs = {
        "x_min" : 4,
        "x_max" : 9,
        "fill_opacity" : 0.75,
        "stroke_width" : 0.25,
    }
    self.graph=graph
    iteraciones=6


    self.rect_list = self.get_riemann_rectangles_list(
        graph, iteraciones,start_color=PURPLE,end_color=ORANGE, **kwargs
    )
    flat_rects = self.get_riemann_rectangles(
